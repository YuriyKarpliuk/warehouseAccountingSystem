<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="564.0" prefWidth="848.0" style="-fx-background-color: #F5DEB3;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="yurii.karpliuk.warehouseAccounting.controllers.SuppliersController">
   <children>
      <Label layoutX="62.0" layoutY="56.0" text="Search suppliers" textFill="#1310cc">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="suppliersSearch" layoutX="271.0" layoutY="56.0" prefHeight="38.0" prefWidth="542.0" promptText="Keywords...">
         <font>
            <Font size="16.0" />
         </font>
      </TextField>
      <TableView fx:id="suppliersTable" layoutX="62.0" layoutY="108.0" prefHeight="368.0" prefWidth="750.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="100.0" text="ID" />
            <TableColumn fx:id="firstName" prefWidth="150.0" text="First name" />
            <TableColumn fx:id="lastName" prefWidth="150.0" text="Last name" />
            <TableColumn fx:id="username" prefWidth="150.0" text="Username" />
            <TableColumn fx:id="email" prefWidth="200.0" text="Email" />
         </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="346.0" layoutY="495.0" mnemonicParsing="false" onAction="#backButtonOnAction" prefHeight="38.0" prefWidth="175.0" style="-fx-background-color: #1310cc;" text="Back" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="RefreshButton" layoutX="637.0" layoutY="495.0" mnemonicParsing="false" onAction="#refreshButtonOnAction" prefHeight="38.0" prefWidth="175.0" style="-fx-background-color: #1310cc;" text="Refresh" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</AnchorPane>
